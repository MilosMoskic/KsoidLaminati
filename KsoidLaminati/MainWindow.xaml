<Window x:Class="KsoidLaminati.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/exression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KsoidLaminati"
        mc:Ignorable="d"
        Title="MainWindow"
        WindowState="Maximized">
    <Canvas x:Name="CanvasMain"
            Width="{Binding ElementName=MainWindow, Path=ActualWidth}"
            Height="{Binding ElementName=KsoidLaminati.MainWindow, Path=ActualHeight}"
            PreviewMouseMove="CanvasMain_PreviewMouseMove"
            PreviewMouseUp="CanvasMain_PreviewMouseUp">
        <Button Content="Button" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="52" Height="20" Click="Button_Click" Canvas.Left="99" Canvas.Top="63"/>
        <Label Content="Pritisni dugme da dodas laminat" Canvas.Left="34" Canvas.Top="37" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Label Content="Tabela stvari&#xD;&#xA;" Canvas.Left="45" Canvas.Top="95" HorizontalAlignment="Center" Height="28" VerticalAlignment="Top"/>
        <Button Content="Button" Canvas.Left="63" Canvas.Top="123" Click="GoToTable"/>

    </Canvas>
</Window>
